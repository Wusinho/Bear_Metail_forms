
<h1>
New User
</h1>

<%= form_with model: @user do |form| %>

<div>
<%= form.label :username %><br>
<%= form.text_field :username %>
</div>

<div>
<%= form.label :email %><br>
<%= form.text_field :email %>
</div>

<div>
<%= form.label :password %><br>
<%= form.text_field :password %>
</div>

<div>
<br>
<%= form.submit %>
</div>

<% end %>